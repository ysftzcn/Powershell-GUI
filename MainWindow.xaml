<Window x:Class="Disabled_Users.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Disabled_Users"
        mc:Ignorable="d"
        Title="Disabled Users Search" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtResults" HorizontalAlignment="Left" Height="333" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="772" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="btnQuery" Content="Search Users" HorizontalAlignment="Left" Margin="626,369,0,0" VerticalAlignment="Top" Width="156" Height="30"/>
        <TextBox x:Name="txtComputer" HorizontalAlignment="Left" Height="30" Margin="382,369,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="199"/>

    </Grid>
</Window>
